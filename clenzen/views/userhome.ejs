            <div class="row">
                <div class="col-lg-12">
                    <div class="panel panel-default">
                        <div class="panel-heading">
                            Order Status
                        </div>
                        <!-- /.panel-heading -->
                        <div class="panel-body">
                            <div class="table-responsive">
                                <table class="table table-striped table-bordered table-hover" id="dataTables-example">
                                    <thead>
                                        <tr>
                                            <th>OrderID</th>
                                            <th>ServiceProviderID</th>
                                            <th>OrderStatus</th>
                                            <th>Rooms</th>
                                            <th>Date</th>
                                            <th>ExtraServices</th>
                                            <th>Charges</th>
                                            <th>Update Status</th>
                                        </tr>
                                    </thead>
                                    <tbody>
                                        <% for(var i=0;i<order.length;i++) {%>
                                        <tr class="odd gradeX">
                                            <td><%=order[i].order_Id%></td>
                                            <td><%=order[i].sp_Id%></td>
                                            <td><%=order[i].order_Activity%></td>
                                            <td>Bed:<%=order[i].bedroom_count%><br />Bath:<%=order[i].bathroom_count%></td>
                                            <td><%=order[i].Service_Date%></td>
                                            <td><%=order[i].extra_services%></td>
                                            <td><%=order[i].Charges%></td>
                                            <td>
                                             <form role="update" method="post" action="updatestatus">
                                             <div class="form-group">
                                             <select placeholder="UpdateStatus" name="ustat">
                                              <option value="ServiceRequested">ServiceRequested</option>
                                              <option value="ServiceCompleted">ServiceCompleted</option>
                                              <option value="RequestRevert">RequestRevert</option>
                                              </select>
                                               </div>
                                               <button type="submit" class="theme_button">Update</button>
                                               </form>
                                      </td>
                                        </tr>
                                        <%}%>
                                    </tbody>
                                </table>
                            </div>
                            <!-- /.table-responsive -->
                            <div class="well">
                                <h4>&nbsp;</h4>
                            </div>
                        </div>
                        <!-- /.panel-body -->
                    </div>
                    <!-- /.panel -->
                </div>
                <!-- /.col-lg-12 -->
            </div>
            <!-- /.row -->
            <div class="row">
            
                <div class="col-lg-6">
                <label>FeedBack Form</label>
                                <form role="form" method="post" name="feedback" id="feedback" post="feedback" action='feedback'>
                                         
                                         <div class="form-group">
                                           <input name="orid" class="form-control" placeholder="Paste OrderID from above">
                                        </div>
                                        <div class="form-group">
                                           <input name="spsid" class="form-control" placeholder="Paste ServiceProviderID from above">
                                        </div>
                                        <div class="form-group">
                                            <label>FeedBack</label>
                                            <textarea name="textfeed" rows="4" class="form-control"></textarea>
                                        </div>
                                                                                                                        
                                        <div class="form-group">
                                            <label>Rating</label>
                                            <select name="rating" class="form-control">
                                                <option value="5">Excellent</option>
                                                <option value="4">Very Good</option>
                                                <option value="3">Fair</option>
                                                <option value="2">Poor</option>
                                                <option value="1">Not Satisfied</option>
                                                <option value="0">Iwant my money back</option>
                                            </select>
                                        </div>
                                        <button class="btn btn-default" type="submit">Submit Button</button>
                                        <button class="btn btn-default" type="reset">Reset Button</button>
                                    </form>
                                 <br />
                                 <br />
                                 <br />
                                 <br />
                                 <center>CleanZen</center>
                                </div>
                                <!-- /.col-lg-6 (nested) -->
                            </div>
                            <!-- /.row (nested) -->
                        </div>
